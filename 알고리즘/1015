                      /* user code */
                      function solution(numbers) {
                         let answer = numbers
                            .map((n) => n + "")       â­ ë¬¸ìì—´ë¡œ ë§Œë“¤ì–´ì•¼, ë”í•  ë•Œ ì‚¬ì¹™ì—°ì‚°ìœ¼ë¡œ ë”í•´ì§€ì§€ ì•Šê³  ë¬¸ìê°’ì— ëŒ€í•œ í•©ìœ¼ë¡œ ì¸ì •ì´ ëœë‹¤.  
                            .sort((x, y) => (y + x) - (x + y))
                            .join("");
                            
                         return answer.split(0).join("") ? answer : "0";
                      }
              
            
