                      /* user code */
                      function solution(numbers) {
                         let answer = numbers
                            .map((n) => n + "")
                            .sort((x, y) => (y + x) - (x + y))
                            .join("");
                            
                         return answer.split(0).join("") ? answer : "0";
                      }
              
            
