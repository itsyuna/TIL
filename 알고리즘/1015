                  /* user code */
                  function solution(n, times) {
                     let high = n * Math.max.apply(null, times);        â­ timesì—ì„œ ê°€ì¥ í° ê°’ì„ ë°˜í™˜í•˜ê³ , ê·¸ ê°’ì„ nìœ¼ë¡œ ê³±í•œê²ƒì„ highë¡œ ì„¤ì •.
                     let low = 0;                                       â­ low = 0ìœ¼ë¡œ ì„¤ì •í•´ì„œ, 
                                                                            ì´ ì‚¬ì´ì— ì´ë¶„ íƒìƒ‰, ê³„ì† ë°˜ìœ¼ë¡œ ì¤„ì—¬ë‚˜ê°€ë©´ì„œ ì›í•˜ëŠ” ë‹µì„ ì°¾ì•„ ë‚˜ê°„ë‹¤.                                       
                     let mid, pass;
                     
                     while (low <= high) {
                        mid = Math.floor(low + high) / 2);
                        pass = times.reduce((sum, time) => (sum += Math.floor(mid / time)), 0);     â­ ë°°ì—´ì„ ëª¨ë‘ ìˆœíšŒí•˜ë©´ì„œ, 
                                                                                                        mid / timesë¥¼ í†µí•´ì„œ ì´ ì‹œê°„ë™ì•ˆ
                                                                                                        ì´ ë¶€ìŠ¤ì— ìˆëŠ” ì‹¬ì‚¬ê´€ì´ ì •í•´ì§„ ì‹œê°„ë™ì•ˆ ëª‡ëª…ì„ ì²˜ë¦¬í•  ìˆ˜ ìˆëŠ”ì§€   
                                                                                                        ê°’ì´ ë‚˜ì˜¨ë‹¤.      -> (* Math.floor()ë¶€ë¶„)
                                                                                                        ê·¸ëŸ¼ ì´ê±¸ ëˆ„ì (* sum+= ë¶€ë¶„)í•´ì„œ
                                                                                                        pass(=ì´ ì²˜ë¦¬í•  ìˆ˜ ìˆëŠ” ì¸ì›)ë¼ëŠ” ë°°ì—´ì„ ì—…ë°ì´íŠ¸ í•œë‹¤.                                            
                        
                        if (n <= pass) high = mid -1;       â­ ì²˜ë¦¬í•  ìˆ˜ ìˆëŠ” ê²½ìš°, mid - 1ë¡œ ì¢€ ë” ë²”ìœ„ë¥¼ ì¢í˜€ë‚˜ê°„ë‹¤.
                        else low = mid + 1;                 â­ ì²˜ë¦¬í•  ìˆ˜ ì—†ëŠ” ê²½ìš°, mid + 1ë¡œ ê³„ì† ë°˜ë³µí•´ ë‚˜ê°„ë‹¤. 
                     }                                                                    
                     
                     return low;                            
                   }
               
               
