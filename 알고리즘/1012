                 /* user code */
                 function solution(n) {
                    let coin = [500, 100, 50, 10, 5, 1];
                    let answer = 0;
                    n = 1000 - n;
                    
                    for (let i = 0; i < 6; i++) {
                       while (n >= coin[i]) {
                          n -= coin[i];
                          answer++;
                       }
                    }
                    
                    return answer;
                 }
                 
                 console.log(solution(380));            // 4
                 console.log(solution(1));              // 15
           
          
