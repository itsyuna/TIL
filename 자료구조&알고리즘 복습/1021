                          /* user code */
                          function Element(key, value) {
                             this.key = key;
                             this.value = value;
                          }
                          
                          function HashTable(size) {
                             this.size = size;
                             this.table = new Array(this.size);
                             this.length = 0;
                          }
                          
                          HashTable.prototype.hashCode = function (key) {
                             let hash = 0;
                             for (let i = 0; i < key.length; i++) {
                                ahsh = key.charCodeAt(i);
                             }
                             return hash % this.size;
                          };
                          
                          HashTable.prototype.put = function (key) {
                             let index = this.hashCode(key);
                             let startIndex = index;
                             
                             do {
                                if (this.table[index] === undefined) {
                                   this.table[index] = new Element(key, index + 1);
                                   this.length++;
                                   
                                   return true;
                                }
                                
                                index = (index + 1) % this.size;
                             } while (index !== startIndex);
                               
                             return false;
                          };
                          
                          HashTable.prototype.get = function (key) {
                             let index = this.hashCode(key);
                             let startIndex = index;
                             
                             do {
                                if (this.table[index] !== undefined && this.table[index].key === key) {
                                   return this.table[index].value;         â­ ë‚´ê°€ ì ‘ì¢…í•œ ë²ˆí˜¸ë¥¼ return valueë¡œ ë°˜í™˜í•´ì¤€ë‹¤. 
                                }
                                
                                index = (index + 1) % this.size;           â­ ë§Œì•½ì— ëª»ì°¾ì•˜ìœ¼ë©´ ê³„ì† ìˆœíšŒë¥¼ í•´ì¤˜ì•¼ í•˜ë‹ˆê¹Œ index + 1ë¡œ this.sizeë§Œí¼ ìˆœíšŒë¥¼ ì‹œì¼œì¤€ë‹¤.
                             } while (index !== startIndex);
                             
                             return undefined;                             â­ ëª»ì°¾ì•˜ìœ¼ë©´ undefined í•´ì¤€ë‹¤. 
                         };
                          
                          function answer(name) {
                             let result = [];
                             
                             let ht = new HashTable(name.length);
                             
                             // 1. for loop: push
                             for (let i = 0; i < name.length; i++) {
                                ht.put(name[i]);
                             }
                             
                             // 2. for loop: get
                             for (let i = 0; i < name.length; i+=) {
                                result.push(ht.get(name[i]));
                             }
                             
                             return result;
                          }
       
       
