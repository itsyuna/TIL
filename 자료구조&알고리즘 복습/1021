                          /* user code */
                          const HASH_SIZE = 21;
                          function HashTable() {
                             this.table = new Array(HASH_SIZE);
                          }
                          
                          HashTable.prototype.hashCode = function (key) {
                             return (key + 10) % HASH_SIZE;           â­ -10 ~ 10 -> 0 ~ 20      : ë²”ìœ„ë¥¼ -10 ~ 10ì—ì„œ 0 ~ 20ìœ¼ë¡œ ë§Œë“¤ì–´ì¤€ë‹¤. 
                                                                                                    0ì—ì„œ 20ì— ëŒ€í•œê±°ëŠ” ë°°ì—´ì˜ ì¸ë±ìŠ¤ë¡œ ì‚¬ìš©í•  ìˆ˜ ìˆê¸° ë•Œë¬¸ì—
                                                                                                    (key)ë¥¼ cardì— ëŒ€í•œ ìˆ«ìë¡œ ì¤¬ê³ , 
                                                                                                    ê·¸ê±°ì— ëŒ€í•œ ì¸ë±ìŠ¤ ë³€í™”ë¥¼ ì´ë ‡ê²Œ ë³´ì •ì²˜ë¦¬ë¥¼ í•´ì¤¬ë‹¤.
                                                                                                    
                                                                                                    ì‚¬ì‹¤ Modular ì—°ì‚°ì„ ì•ˆí•´ë„ ë˜ëŠ”ë°, 
                                                                                                    ì´ëŸ° ë¬¸ì œê°€ ëŒ€ë¶€ë¶„ ì—„ì²­ë‚˜ê²Œ í° ìˆ«ìë¥¼ ì£¼ê³ 
                                                                                                    ì •í•´ì ¸ìˆëŠ” ë°°ì—´ ì‚¬ì´ì¦ˆë‚´ì—ì„œ 
                                                                                                    íš¨ìœ¨ì ìœ¼ë¡œ ì˜ counting í•  ìˆ˜ ìˆëŠ”ì§€ë¥¼ ë¬¼ì–´ë³´ëŠ” ë¬¸ì œê°€ ëŒ€ë‹¤ìˆ˜ì´ê¸° ë•Œë¬¸ì—
                                                                                                    ê·¸ëŸ° í™•ì¥ì„±ìœ¼ë¡œ ì¸í•´ì„œ Modular ì—°ì‚°ì„ ì…‹íŒ…í•´ì¤¬ë‹¤. 
                         };                                                                                                                               
                          
                         HashTable.prototype.put = function (key) {
                            let index = this.hashCode(key);
                            if (this.table[index] === underfined) {
                               this.table[index] = 0;
                            }
                           this.table[index]++;
                         };
                          
                         HashTable.prototype.get = function (key) {
                            let index = this.hashCode(key);
                            return this.table[index] === underfined ? - : this.table[index];
                         };
                          
                         function answer(card, select) {
                            let result = [];
                             
                            // 1. cardë‚´ ìˆëŠ” ìˆ«ì ì¹´ë“œê°€ ëª‡ê°œì¸ì§€ë¥¼ count
                            let ht = new HashTable();
                            for (let i = 0; i < card.length; i++) {
                               â­ ì¹´ë“œ ìˆ«ì ë³„ count, 10 -> 2, -10 -> 3
                               ht.put(card[i]);
                            }
                             
                            // 2. selectë‚´ ìˆëŠ” ìˆ«ì ì¹´ë“œê°€ 1ë²ˆì˜ countí•œ ë°°ì—´ì—ì„œ ëª‡ ê°œ ìˆëŠ”ì§€ í™•ì¸
                            for (let i = 0; i < select.length; i++) {
                               â­ 1ë²ˆì—ì„œ êµ¬í•œ ìˆ«ì ì¹´ë“œê°€ ìˆëŠ” ê²½ìš°, ê·¸ ì¹´ë“œì˜ ê°œìˆ˜ë¥¼ ë„£ì–´ì£¼ê³ , ì—†ëŠ” ê²½ìš°ëŠ” 0 
                               result.push(ht.get(select[i]));
                            }
                       
                            return result;
                         }
                          
                         /* main code */
                         let input = [
                            // TC: 1
                            [
                              [-6, -1, 6, 1, 1], 
                              [-2, 1, 3],
                            ],
                             
                            // TC: 2
