                        function solution(phone_number) {
                            var answer = '';

                            // 1. ë’· 4ìë¦¬ ì œì™¸í•œ ë‚˜ë¨¸ì§€ ìˆ«ìë¥¼ ë¨¼ì € ì¶”ì¶œ.
                            answer = phone_number.slice(0, phone_number.length - 4);

                            // 2. ê·¸ ìˆ«ìë“¤ì„ ëª¨ë‘ *ë¡œ ë°”ê¿”ì£¼ê¸°.
                            for (let i = 0; i < answer.length; i++) {
                                answer = answer.replace(answer[i], '*');
                            }

                            // 3. *ë¡œ ë°”ê¿”ì¤€ ìˆ«ìë“¤ê³¼, ë‚˜ë¨¸ì§€ ë’· 4ìë¦¬ í•©ì¹˜ê¸°.
                            answer = answer + phone_number.slice(phone_number.length-4);

                            return answer;
                        }


                   ğŸ”» ë‹¤ë¥¸ ì‚¬ëŒì´ í’€ì´í•œ ë°©ë²• (1)       â­ ì •ê·œì‹ ë°©ë²•
                   
                        function hide_numbers(s) {
                          return s.replace(/\d(?=\d{4})/g, "*");
                        }

                        // í…ŒìŠ¤íŠ¸ ì½”ë“œ
                        console.log("ê²°ê³¼ : " + hide_numbers('01033334444'));
                
                        ğŸ’¡ ë‚´ê°€ ìŠ¤ìŠ¤ë¡œ í’€ì—ˆì„ ë•ŒëŠ”, ì •ê·œì‹ì„ ì•Œê³  ìˆì—ˆëŠ”ë°ë„ ë¬¸ì œí’€ì´ì— í™œìš©í•˜ì§€ ëª»í–ˆë‹¤. 
                           ì •ê·œì‹ì„ ì‚¬ìš©í•˜ë©´ ì´ë ‡ê²Œë‚˜ ì½”ë“œê°€ í•œ ì¤„ë¡œ ê°„ë‹¨í•´ì§„ë‹¤.
