                  function solution(s) {
                      var answer = ''; 

                      // 1. ë¨¼ì € ê³µë°±ì„ ê¸°ì¤€ìœ¼ë¡œ ë‹¨ì–´ ë‚˜ëˆ„ê¸°.
                      var tmp = s.split(" ");     

                      for (var i in tmp) {
                          for (var j in tmp[i]) {
                              // 2. ì§ìˆ˜ë²ˆì§¸ ì¸ë±ìŠ¤ëŠ” ëŒ€ë¬¸ìë¡œ ë³€í™˜.
                              if (j % 2 === 0) {
                                  answer += tmp[i][j].toUpperCase();
                              } else {    // 3. í™€ìˆ˜ë²ˆì§¸ ì¸ë±ìŠ¤ëŠ” ì†Œë¬¸ìë¡œ ë³€í™˜.
                                  answer += tmp[i][j].toLowerCase();
                              }
                          }       
                         answer += " ";       // ë‹¨ì–´ê°€ í•˜ë‚˜ ì´ìƒì˜ ê³µë°±ë¬¸ìë¡œ êµ¬ë¶„ë˜ì–´ ìˆìœ¼ë‹ˆê¹Œ, answerì— ë”í•´ì¤„ ë•Œ ê³µë°±ì„ ì¶”ê°€í•´ì„œ ë”í•´ì¤€ë‹¤. 
                      }

                      return answer.slice(0, -1);
                  }
                  
