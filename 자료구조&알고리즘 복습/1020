                            /* user code */
                            if (!Array.prototype.peek) {
                               Array.prototype.peek = function () {
                                  return this[this.length - 1];
                               };
                            }
                            
                            if (!Array.prototype.isEmpty) {
                               Array.prototype.isEmpty = funciton () {
                                  return this.length == 0;
