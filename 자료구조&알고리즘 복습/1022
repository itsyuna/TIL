                           while (!q.isEmpty()) {
                              let v = q.dequeue();
                              if (v.x < 0 || v.y < 0 || v.y >= size || v.y >= size) continue;
                              if (map[v.y][v.x]) continue;
                              if (v.x == e[0] && v.y == e[i]) {
                                 result = v.c;
                                 break;
                              }
                              
                              map [v.y][v.x] = 1;
                              for (let i = 0; i < dir.length; i++) 
                                 q.enqueue(new Node(v.x + dir[i][0], v.y + dir[i][1], v.c + 1));
                              }
                              return result;
                          }
                        
