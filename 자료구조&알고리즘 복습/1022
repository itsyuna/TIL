                         /* user code */
                         function Node(value) {
                            this.value = value;
                            this.left = null;
                            this.right = null;
                         }
                         
                         function BinaryTree( {
                            this.root = null;
                         }
                         
                         BinaryTree.prototype._insertNode = function (node, value) {
                            if (node === null) {
                               node = new Node(value);
                            } else if (value < node.value) {
                               node.left = this._insertNode(node.left, value);
                            } else if (value >= node.value) {
                               node.right = this._insertNode(node.right, value);
                            }
                            return node;
                         };
                         
                         BinaryTree.prototype.insert = function (value) {         â­ Valueë§Œ ë°›ì•„ì„œ
                            this.root = this._insertNode(this.root, value);       â­ -> this.rootì— ì—…ë°ì´íŠ¸ ëœ Nodeì •ë³´ë¥¼ ë‹¤ì‹œ ë°˜í™˜ì‹œì¼œì¤€ë‹¤.  
                         };
                         
                         // í›„ìœ„ ìˆœíšŒí•˜ë©´ì„œ í›„ìœ„ ìˆœíšŒí•œ ê²°ê³¼ê°’ì„ resultì— ì—…ë°ì´íŠ¸ ì‹œì¼œì¤˜ì•¼ í•œë‹¤. 
                         BinaryTree.prototype.postOrderTraverse = function (node, array) {
                            if (node === null) {
                               return;
                            }
                            
                            this.postOrderTraverse(node.left, array);
                            this.postOrderTraverse(node.right, array);
                         };
                         
                         function answer(str) {
                            let result = [];
                            
                            let tree = new BinaryTree();
                            for (let i = 0; i < str.length; i++) {
                               tree.insert(str[i]);
                            }
                            
                            tree.postOrderTraverse(tree.root, result);
                            
                            return result;
                         }
                       
                         /* main code */
                         let input = [
                            // TC: 1
                            "ABC",
                            
                            // TC: 2
                            "FBADCEGIH",
                            
                            // TC: 3
                            "CBAEDFG",
                         ];
                         
                         for (let i = 0; i < input.length; i++) {
                            process.stdout.write(`#${i + 1} `);
                            console.log(answer(input[i]));
                         }
            
                   
