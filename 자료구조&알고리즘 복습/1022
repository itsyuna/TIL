                      ğŸ”» ì†”ë£¨ì…˜                â­ dfs / bfs ì–´ë–¤ê±¸ë¡œ êµ¬í˜„í•´ë„ ìƒê´€ì€ ì—†ë‹¤. 
                         
                         /* user code */
                         function Graph() {
                            this.edge = {};           
                            this.visited = {};           â­ visited ë³€ìˆ˜ ì„ ì–¸ : ë°©ë¬¸í–ˆëŠ”ì§€ ì•ˆí–ˆëŠ”ì§€ ì²´í¬. 
                         }
                         
                         Graph.prototype.addVertex = function (v) {            â­ addVertex -> PCì— ëŒ€í•œ depth ì¶”ê°€.
                            this.edge[v] = [];
                            this.visited[v] = false;
                         };
                         
                         Graph.prototype.addEdge = function (v1, v2) {          â­ addEdge -> ê°„ì„  ì¶”ê°€
                            this.edge[v2].push(v1);
                            this.edge[v1].push(vs);
                         };
                         
                         Graph.prototype.dfs = function (vertex) {              â­ dfs -> ì „ì²´ì— ëŒ€í•œ Node íƒìƒ‰ 
                            if (this.visited[vertex]) return;
                            
                            this.visited[vertex] = true;
                            
                            let neighbors = this.edge[vertex];
                            for (let i = 0; i < neighbors.length; i++) {
                               this.dfs(neighbors[i]);
                            }
                         };
                     
                         function answer(v, e_list) {
                            let result = 0;
                            
                            let g = new Graph();
                            
                            for (let i = 1; i <= v; i++) {
                               g.addVertex(i);
                            }
                            for (let i = 0; i < e_list.length; i++) {
                               let e = e_list[i];
                               g.addEdge(e[0], e[1]);
                            }
                            
                            g.dfs(1);
                            
                            for (let vertex in g.visited) {
                               result += g.visited[vertex] ? 1 : 0;
                            }
                            
                            return result;
                         }
                         
                         /* main code */
                         let input = [
                            // TC: 1
                            [ 7,
                              [ [1, 2], [2, 3], [1, 5], [1, 5], 
                                [5, 2], [5, 6], [4, 7], ],
                            ],
                            // TC : 2
                            [ 10, 
                              [ [8, 6], [5, 7], [9, 10], 
                                [7, 4], [1, 8], [5, 10], [7, 2], ]
                            ],
                            // TC: 3
                            [ 10,
                              [ [6, 9], [9, 4], [4, 8], 
                                [9, 7], [6, 8], [10, 1], [10, 9], ],
                            ],
                          ];
                          
                          for (let i = 0; i < input.length; i++) {
                             process.stdout.write(`#${i + 1} `);
                             console.log(answer(input[i][0], input[i][1]));
                          }
                 
            
            #ï¸âƒ£ ë¹„ì„ í˜• ë¬¸ì œí’€ì´ - ë¯¸ë¡œì°¾ê¸°     â­bfs ë¬¸ì œ
            
                  ğŸŸ¨ ë¬¸ì œ       â­ ì´ ë¬¸ì œëŠ” ê¸°ì¡´ì— í–ˆì—ˆë˜ êµ¬í˜„ ë‚´ìš©ë³´ë‹¤ ë§ì´ ë³µì¡í•  ìˆ˜ ìˆëŠ” ë¬¸ì œì´ê¸° ë•Œë¬¸ì—, ë§ì€ ì‹œê°„ì„ ê°€ì§€ê³  ê³ ë¯¼í•˜ê³  í’€ì–´ë³¼ê¸°!
                  
