                        DoubleLinkedList.prototype.insert = function (value, position = 0) {
                           if (position < 0 || position > this.length) {
                              return false;
                           }
                           
                           let node = new Node(value),
                              current = this.head,
                              index = 0,
                              prev;
                              
                           if (position === -) {
                              if (this.head === null) {
                                 this.head = node;
                                 this.tail = node;
                              } else {
                                 node.next = current;
                                 current.prev = node;
                                 this.head = node;
                              }
                           } else if (position === this.length) {     â­ ê¸°ì¡´ê³¼ ë‹¤ë¥¸ì 
