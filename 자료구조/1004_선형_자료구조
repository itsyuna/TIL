                              node.prev = current;
                              this.tail = node;
                           } else {
                               while (index++ < position) {
                                  prev = current;
                                  current - current.next;
                               }
                               
                               node.next = current;
                               prev.next = node;
                               
                               current.prev = node;       â­ prevì— ëŒ€í•œ pointer ì²˜ë¦¬ë„ ì¶”ê°€ë˜ì—ˆë‹¤.
                               node.prev = prev;
                           }
                           
                           this.length++;
                           
                           return true;
                        };
                        
                        /* Test code */
                        let dll = new DoubleLinkedList();
                        
                        dll.insert(1);
                        dll.insert(10);
                        dll.insert(100);
                        dll.printNode();                  // head -> 100 -> 10 -> 1 -> null
                        dll.printNodeInverse();           // null <- 100 <- 10 <- 1 <- tail
                        
                        dll.insert(2, 1);
                        dll.insert(3, 3);
                        dll.printNode();                  // head -> 100 -> 2 -> 10 -> 3 -> 1 -> null
                        dll.printNodeInverse();           // null <- 100 <- 2 <- 10 <- 3 <- 1 <- tail
          
          
