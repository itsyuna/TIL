                        // Node(): dataì™€ pointë¥¼ ê°€ì§€ê³  ìˆëŠ” ê°ì²´
                        function Node(data) {
                            this.data = data;
                            this.next = null;
                        }
                        
                        // LinkedList(): headì™€ lengthë¥¼ ê°€ì§€ê³  ìˆëŠ” ê°ì²´
                        function LinkedList() {
                            this.head = null;
                            this.length = 0;
                        }
                        
                        // size(): ì—°ê²° ë¦¬ìŠ¤íŠ¸ ë‚´ ë…¸ë“œ ê°œìˆ˜ í™•ì¸
                        LinkedList.prototype.size = function () {
                            return this.length;
                        };
                        
                        // is Empty(): ê°ì²´ ë‚´ ë…¸ë“œ ì¡´ì¬ ì—¬ë¶€ íŒŒì•…
                        LinkedList.prototype.isEmpty = function () {
                            return this.length === 0;
                        };
                        
                        /* Test code */
                        let ll = new LinkedList();
                        console.log(ll);                        // LinkedList { head: null, length: 0 }
                        
                        ll.head = new Node(123);
                        ll.length++;
                        console.log(ll);                        // LinkedList { head: Node { data: 123, next: null }, length: 1 }
                        
                        ll.head.next = new Node(456);          
                        ll.length++;
                        console.log(ll);                       // LinkedList { head: Node { data: 123, next: Node { data: 456, next: null } },
                                                                   length: 2 }
                        
                        
