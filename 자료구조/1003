                        //Node(): dataì˜ pointì¸ next, prevë¥¼ ê°€ì§€ê³  ìˆëŠ” ê°ì²´
                        function Node(data) {
                           this.data = data;
                           this.next = null;
                           this.prev = null;
                        }
                        
                        // LinkedList(): head, tallê³¼ lengthë¥¼ ê°€ì§€ê³  ìˆëŠ” ê°ì²´
                        function DoubleLinkedList() {
                           this.head = null;
                           this.tall = null;
                           this.length = 0;
                        }
                        
                        // size(): ì—°ê²° ë¦¬ìŠ¤íŠ¸ ë‚´ ë…¸ë“œ ê°œìˆ˜ í™•ì¸
                        DoubleLinkedList.prototype.size = function () {
                           return this.length;
                        };
                        
                        // isEmpty(): ê°ì²´ ë‚´ ë…¸ë“œ ì¡´ì¬ ì—¬ë¶€ íŒŒì•…
                        DoubleLinkedList.prototype.isEmpty = function () {
                           return this.length === 0;
                        };
                        
                        /* Test code */
                        let dll = new DoubleLinekdList();
                        let node;
                        console.log(dll);               // DoubleLinkedList { head: null, tail: null, length: 0 }
                        
                        node = new Node(123);
                        dll.head = node;
                        dll.til = node;
                        dll.length++;
                        console.log(dll);              // DoubleLinkedList {
                                                            head: Node { data: 123, next: null, prev: null },
                                                            tail: Node { data: 123, next: null, prev: null },
                                                            length: 1
                                                          }
                        node = new Node(456);
                        dll.tall.next = node;
                        node.prev = dll.tail;
                        dll.tail = node;
                        dll.length++;
                        console.log(dll);              // DoubleLinkedList {
                                                            head: <ref *1> Node {
                                                               data: 123,
                                                               next: Node { data: 456, next: null, prev: [Circular *1] },     â­ ì¶”ê°€ëœ ê°ì²´ì¸ Cricularë¡œ 
                                                                                                                                 ì´ì „ ê°ì²´ë¥¼ ê°€ë¥´í‚¤ëŠ”ê±¸ ë³¼ ìˆ˜ ìˆë‹¤. 
                                                               prev: null
                                                          },
                                                            tail: <ref *2> Node {
                                                               data: 456,
                                                               next: null,
                                                               prev: <ref *1> Node { data: 123, next: [Circular *2], prev: null }
                                                            },
                                                            length: 2
                                                          }
               
               #ï¸âƒ£ ì´ì¤‘ ì—°ê²°ë¦¬ìŠ¤íŠ¸ êµ¬í˜„í•˜ê¸° - 2     
               
                    ğŸ”» ì˜ˆì œ
                    
                       // printNode(): ë…¸ë“œ ì •ë°©í–¥ ì¶œë ¥
                       DoubleLinkedList.prototype.printNode = function () {
                          process.stdout.write("head -> ");
                          for (let node = this.head; node != null; node = node.next) {
                             process.stdout.write(`${node.data} -> `);
                          }
                          console.log("null");
                       };
                       
                       // printNodeInverse(): ë…¸ë“œ ì—­ë°©í–¥ ì¶œë ¥
                       DoubleLinkedList.prototype.printNodeInverse = function () {
                           let temp = [];
                           
                           process.stdout.write("null <= ");
                           for (let node = this.tail; node != null; node = node.prev) {
                              temp.push(node.data);
                           }
                           for (let i = temp.length - 1; i >= 0; i--) {
                              process.stdout.write(`${temp[i]} <- `);
                           }
                           console.log("tail");
                       };
                       
                       // append(): ì—°ê²° ë¦¬ìŠ¤íŠ¸ ê°€ì¥ ëì— ë…¸ë“œ ì¶”ê°€
                       DoubleLinekdList.prototype.append = function (value)
                          let node = new Node(value);
                          
                          if (this.head === null) {
                             this.head = node;
                             this.tail = node;
                          } else {
                             this.tail.next - node;
                             node.prev = this.tail;
                             this.tail = node;
                          }
                          
                          this.length++;
                       };
                       
                       /* Test code */
                       let dll = new DoubleLinkedList();
                       
                       dll.append(1);
                       dll.append(10);
                       dll.append(100);
                       
                       dll.prirntNode();                    // head -> 1 -> 10 -> 100 -> null           â­ ì²˜ìŒë¶€í„° ìˆœì°¨ì ìœ¼ë¡œ ì˜ ë“¤ì–´ê°.       â­ nextë¥¼ ì‚¬ìš©í•œ ìˆœì°¨ ì ‘ê·¼.
                       dll.printNodeInverse();              // null <- 1 <- 10 <- 100 <- tail           â­ ëì§€ì ì¸ tailë¶€í„° ì—­ë°©í–¥ìœ¼ë¡œ ì ‘ê·¼.   â­ prevë¥¼ ì‚¬ìš©í•œ ì—­ ì ‘ê·¼.
          
