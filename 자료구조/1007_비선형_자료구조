                    // each(): ëª¨ë“  ê°œì²´ ìš”ì†Œì— ëŒ€í•´ callback í•¨ìˆ˜ ìˆ˜í–‰ (:= foreach)
                    Dictionary.prototype.each = function (fn) {
                       for (let k in this.items) {
                          if (thi.s.has(k) {
                             fn(k, this.items[k]);
                          }
                       }
                    };
                    
                    // printDictionary(): ê°œì²´ ì¶œë ¥ callback
                    function printDictionary(key, value) {
                       console.log(`key: ${key}`);
                       console.log(`value: ${value}`);
                    }
                    
                    let dict = new Dictionary();
                    dict.set("age", 19);
                    dict.set("name", "alice");
                    dict.set("height", 172);
                    console.log(dict);
                    
                    console.log(dict.keys());                 // [ 'name', 'height' ]
                    console.log(dict.values());               // [ 'alice', 172 ]
                    dict.each(printDictionary);               // key: nmae
                                                                 value: alice
                                                                 key: height
                                                                 value: 172
                    
                  
