                                     let fruits_add = ["cherry", "banana"];
                                     
                                     console.log(fruits.concat(fruits_add));      // [ 'apple', 'orange', 'melon', 'cherry', 'banana' ]
                                                                                    
                                     console.log(fruits);                         // [ 'apple', 'orange', 'melon' ]
                                     
