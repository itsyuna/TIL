                    function factorial(num) {
                       if (num == 0) return 1;      â­ 0ì„ ê³±í•˜ë©´, ë‹¤ 0ì´ ë˜ë²„ë¦¬ë‹ˆê¹Œ 1ì„ í•´ì¤€ë‹¤. 
                       return num * factorial(num - 1);
                    }
                    // 3 * 2 * 1 * 0 -> 6
                    // 4 * 3 * 2 * 1 * 0 -> 24      
                    
                    console.log(factorial(3));      // output: 6
                    console.log(factorial(4));      // output: 24
                
